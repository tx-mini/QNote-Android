<?xml version="1.0" encoding="utf-8"?>
<litepal>

    <dbname value="qnote" />

    <version value="4" />

    <!--
    	Define your models in the list with mapping tag, LitePal will
    	create tables for each mapping class. The supported fields
    	defined in models will be mapped into columns.
    	For example:
    	<list>
    		<mapping class="com.test.model.Reader" />
    		<mapping class="com.test.model.Magazine" />
    	</list>
    -->
    <list>
        <mapping class="csu.edu.ice.model.model.University"/>
        <mapping class="csu.edu.ice.model.model.CustomCourse"/>
        <mapping class="csu.edu.ice.model.dao.RubbishBean"/>
        <mapping class="csu.edu.ice.model.dao.TermBean"/>
        <mapping class="csu.edu.ice.model.dao.BookBean"/>
        <mapping class="csu.edu.ice.model.dao.NoteBean"/>

    </list>
</litepal>